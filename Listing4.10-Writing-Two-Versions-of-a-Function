#include <iostream>
#include <stdlib.h>
#include <string>
using namespace std;

void Combine (string first, string second)
{
  cout << first << " " << second << endl;
}
void Combine (int first, int second)
{
  int sum = first + second;
  cout << first << "+" << second << "=" << sum << endl;
}

int main()
{
  Combine("Bob", "Joel");
  Combine(1, 2);
  return 0;
}
